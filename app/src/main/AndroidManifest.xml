<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jjz.frameworkdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


<application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".MyApplication"
        android:theme="@style/Theme.MyFramework">
        <activity android:name=".ui.viewmodel.VM2Activity"></activity>
        <activity android:name=".ui.viewmodel.ViewModelActivity" />
        <activity android:name=".ui.viewmodel.ViewModelKtActivity" />
        <activity android:name=".ui.viewbinding.java.ViewBindingJavaActivity" />
        <activity android:name=".ui.viewbinding.kotlin.ViewBindingKtActivity" />
        <activity android:name=".ui.HttpRequestActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>